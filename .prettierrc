{
	"arrowParens": "avoid",
	"bracketSpacing": true,
	"trailingComma": "es5",
	"printWidth": 100,
	"singleQuote": true,
	"tabWidth": 3,
	"semi": true,
	"useTabs": true,
	"bracketSameLine": true,
	"embeddedLanguageFormatting": "auto",
	"quoteProps": "as-needed",
	"proseWrap": "preserve",
	"vueIndentScriptAndStyle": false,
	"htmlWhitespaceSensitivity": "css",
	"endOfLine": "lf"
}
